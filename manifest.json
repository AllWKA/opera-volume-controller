{
	"manifest_version": 2,
	"content_scripts": [
		{
			"matches": [
				"<all_urls>"
			],
			"js": [
				"content.js"
			]
		}
	],
	"name": "control tab volume",
	"description": "control tab volume",
	"version": "1.0",
	"background": {
		"scripts": [
			"background.js"
		]
	},
	"permissions": [
		"tabs",
		"desktopCapture",
		"storage"
	],
	"browser_action": {
		"default_title": "Audio Controler",
		"default_popup": "popup.html"
	}
}